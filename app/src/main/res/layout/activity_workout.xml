<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="workoutVM" type="com.evinanshsapplication.app.modules.workout.data.viewmodel.WorkoutVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearWorkout"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_19pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrectangle4207"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackarrowleft"
                android:layout_height="@dimen/_220pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackrectangle4207"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_220pxv" android:layout_width="match_parent">
                    <View android:background="@color/gray_300"
                        android:id="@+id/viewRectangle4207"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_172pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_187_5"
                        android:id="@+id/viewEllipseSeven"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_114pxv"
                        android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                </FrameLayout>
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStyledeep_purple_300">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginBottom="@dimen/_26pxv"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="@dimen/_9pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitle"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_149pxh"
                            android:layout_marginStart="@dimen/_112pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_weight="0.89"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{workoutVM.workoutModel.txtTitle}"
                            style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/lbl_my_app"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:id="@+id/linearRowjonathanborba"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_75pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_75pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnjonathanborba"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.42"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                        <ImageView android:gravity="center_horizontal"
                            android:id="@+id/imageJonathanborba"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_jonathanborba}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_jonathanborba"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTitleOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_weight="0.58"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtTitleOne}"
                        style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/lbl_pratama"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTabs"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTab"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTab"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_64pxh"
                            android:layout_marginStart="@dimen/_64pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{workoutVM.workoutModel.txtTab}"
                            style="@style/txtManropebold14" tools:text="@string/lbl_workout"/>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLine"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_187pxh"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTab1"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTabOne"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_58pxh"
                            android:layout_marginStart="@dimen/_58pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{workoutVM.workoutModel.txtTabOne}"
                            style="@style/txtManroperegular14" tools:text="@string/lbl_meditation"/>
                        <View android:background="@color/blue_50"
                            android:id="@+id/lineLineOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_187pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearCard"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <FrameLayout android:id="@+id/frameThumbnail"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_200pxv" android:layout_width="@dimen/_319pxh">
                    <ImageView android:id="@+id/imageThumbnail"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_200pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_thumbnail}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbnail"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnWorkout"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_26pxv"
                        android:layout_marginHorizontal="@dimen/_20pxh"
                        android:layout_marginVertical="@dimen/_20pxv"
                        android:layout_width="@dimen/_76pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_7pxh"
                        android:paddingStart="@dimen/_7pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_workout2"
                        android:textAllCaps="false"
                        style="@style/btnGradientRounded" tools:text="@string/lbl_workout2"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitleTwo"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{workoutVM.workoutModel.txtTitleTwo}"
                    style="@style/txtManropebold16" tools:text="@string/lbl_workout_title"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLevel"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.26"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtLevel}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl_level"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.04"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtOne}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDuration"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_weight="0.11"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtDuration}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl_12"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMins"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.24"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtMins}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl_mins"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.04"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtTwo}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCalories"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_weight="0.17"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtCalories}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl_123"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCal"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_weight="0.15"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{workoutVM.workoutModel.txtCal}"
                        style="@style/txtManroperegular14" tools:text="@string/lbl_cal"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="match_parent" style="@style/groupStyledeep_purple_300cornerRadius">
            <LinearLayout android:id="@+id/linearRowvector"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_35pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_72pxh"
                    android:layout_width="@dimen/_47pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                <ImageView android:id="@+id/imageVectorTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_35pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_white_a700_31x35"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_31x35"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
